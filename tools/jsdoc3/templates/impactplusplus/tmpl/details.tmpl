<?js
var data = obj;
var self = this;
?>
<dl class="details dl-horizontal">
    <?js
        var properties = data.properties;
        if (properties && properties.length && properties.forEach) { 
    ?>
        
        <h5 class="subsection-title">Properties:</h5>
        
        <dl><?js= this.partial('properties.tmpl', properties) ?></dl>
        
    <?js } ?>
        
    <?js if (data.version) {?>
    <dt class="tag-version">Version:</dt>
    <dd class="tag-version"><ul class="unstyled dummy"><li><?js= version ?></li></ul></dd>
    <?js } ?>
    
    <?js if (data.since) {?>
    <dt class="tag-since">Since:</dt>
    <dd class="tag-since"><ul class="unstyled dummy"><li><?js= since ?></dd>
    <?js } ?>

    <?js if (data.inherited && data.inherits) { ?>
    <dt class="inherited-from">Inherited From:</dt>
    <dd class="inherited-from"><ul class="unstyled dummy"><li>
        <?js= this.linkto(data.inherits, this.htmlsafe(data.inherits)) ?>
    </li></dd>
    <?js } ?>
    
    <?js if (data.deprecated) { ?>
        <dt class="important tag-deprecated">Deprecated:</dt><?js
            if (data.deprecated === true) { ?><dd class="yes-def tag-deprecated"><ul class="unstyled dummy"><li>Yes</li></ul></dd><?js }
            else { ?><dd><ul class="unstyled dummy"><li><?js= data.deprecated ?></li></ul></dd><?js }
        ?>
    <?js } ?>
    
    <?js if (data.author && author.length) {?>
    <dt class="tag-author">Author:</dt>
    <dd class="tag-author">
        <ul class="unstyled"><?js author.forEach(function(a) { ?>
            <li><?js= self.resolveAuthorLinks(a) ?></li>
        <?js }); ?></ul>
    </dd>
    <?js } ?>
    
    <?js if (data.copyright) {?>
    <dt class="tag-copyright">Copyright:</dt>
    <dd class="tag-copyright"><ul class="unstyled dummy"><li><?js= copyright ?></li></ul></dd>
    <?js } ?>
    
    <?js if (data.license) {?>
    <dt class="tag-license">License:</dt>
    <dd class="tag-license"><ul class="unstyled dummy"><li><?js= license ?></li></ul></dd>
    <?js } ?>
    
    <?js if (data.defaultvalue) {?>
    <dt class="tag-default">Default Value:</dt>
    <dd class="tag-default"><ul class="unstyled dummy"><li><?js= data.defaultvalue ?></li></ul></dd>
    <?js } ?>
    
    <?js if (data.meta) {?>
	<dt class="tag-source">Source:</dt>
	<dd class="tag-source"><ul class="unstyled dummy"><li>
		
		<?js if ( data.meta.path ) {
		
			docpath = data.meta.path;
			docfolders = docpath.split( /\/|\\/ );
			docfolder = docfolders[ docfolders.length - 1 ];
			docfilename = data.meta.filename;
			docfileparts = docfilename.split( /\/|\\/ );
			docfilename = docfileparts[ docfileparts.length - 1 ];
			
		?>
		<a href="https://github.com/collinhover/impactplusplus/tree/master/lib/plusplus/<?js= docfolder ?>/<?js= docfilename ?>#L<?js= meta.lineno ?>" target="_blank"><?js= self.linkto(meta.filename) ?>, <?js= self.linkto(meta.filename, 'line ' + meta.lineno, null, 'line' + meta.lineno) ?></a>
		<?js } else {?>
		<?js= self.linkto(meta.filename) ?>, <?js= self.linkto(meta.filename, 'line ' + meta.lineno, null, 'line' + meta.lineno) ?>
		<?js } ?>
	</li></ul></dd>
    <?js } ?>
    
    <?js if (data.tutorials && tutorials.length) {?>
    <dt class="tag-tutorial">Tutorials:</dt>
    <dd class="tag-tutorial">
        <ul class="unstyled"><?js tutorials.forEach(function(t) { ?>
            <li><?js= self.tutoriallink(t) ?></li>
        <?js }); ?></ul>
    </dd>
    <?js } ?>
    
    <?js if (data.see && see.length) {?>
    <dt class="tag-see">See:</dt>
    <dd class="tag-see">
        <ul class="unstyled"><?js see.forEach(function(s) { ?>
            <li><?js= self.linkto(s) ?></li>
        <?js }); ?></ul>
    </dd>
    <?js } ?>
    
    <?js if (data.todo && todo.length) {?>
    <dt class="tag-todo">To Do:</dt>
    <dd class="tag-todo">
        <ul class="unstyled"><?js todo.forEach(function(t) { ?>
            <li><?js= t ?></li>
        <?js }); ?></ul>
    </dd>
    <?js } ?>
</dl>
